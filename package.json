{
  "version": "0.0.0",
  "private": true,
  "description": "Jawis monorepo",
  "scripts": {
    "start": "concurrently --kill-others \"yarn -s javi\" \"yarn -s start:client\"",
    "start:client": "webpack serve -c packages/dev/devClient/webpack.dev.js",
    "": "",
    "lint": "eslint packages",
    "lint:fix": "eslint --fix packages",
    "pretty": "prettier -l \"packages/**/{*.{ts,tsx,js,jsx,json,md,css,html},.prettierrc}\"",
    "pretty:fix": "yarn -s pretty --write",
    "custom:fix": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/lint-fix-all.ts",
    ".": "",
    "compile": "yarn -s tsc -b",
    "recompile": "rimraf build-tsc && yarn -s tsc -b",
    "clean": "rimraf build-tsc",
    "..": "",
    "build": "yarn recompile && yarn build:fast",
    "build:fast": "yarn build:source && yarn build:extra && yarn build:client",
    "build:source": "node packages/dev/scripts/build/build-main.js",
    "build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-jawis.ts",
    "build:client": "webpack -o build/publish/javi/client -c packages/javi-client/webpack.javi.js",
    "...": "",
    "alpha:start": "concurrently --kill-others \"yarn -s alpha:start:alone\" \"yarn -s start:client\"",
    "alpha:start:alone": "echo alpha && node build-alpha/javi/javiServerMain",
    "alpha:build": "yarn recompile && yarn alpha:build:fast",
    "alpha:build:fast": "yarn alpha:build:source && yarn alpha:build:extra && yarn alpha:build:client",
    "alpha:build:source": "node packages/dev/scripts/build/build-alpha-main.js && yarn alpha:build:paths",
    "alpha:build:paths": "yarn -s tsc-alias -p tsconfig.alpha.paths.json",
    "alpha:build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-jawis-alpha.ts",
    "alpha:build:client": "webpack -o build-alpha/javi/client -c packages/javi-client/webpack.javi.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@jawis/console": "^1.0.3",
    "@jawis/jagov": "^2.0.0",
    "@jawis/javi": "^1.0.3",
    "@testing-library/react-hooks": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "concurrently": "^7.2.2",
    "eslint": "^8.44.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.6.11",
    "typescript": "^4.6.2"
  }
}
