{
  "version": "0.0.0",
  "private": true,
  "description": "Jawis monorepo",
  "scripts": {
    "start": "concurrently --kill-others \"yarn -s javi\" \"yarn -s start:client\"",
    "start:client": "webpack serve -c packages/dev/devClient/webpack.dev.js",
    "": "",
    "dev-test": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/devServer/devJaviTestMain.ts",
    "dev-lint-fix": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/hello-project-watcher.ts",
    ".": "",
    "lint": "eslint packages",
    "lint:fix": "eslint --fix packages",
    "pretty": "prettier -l \"packages/**/{*.{ts,tsx,js,jsx,json,md,css,html},.prettierrc}\"",
    "pretty:fix": "yarn -s pretty --write",
    "custom:fix": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/lint-fix-all.ts",
    "..": "",
    "compile": "yarn -s tsc -b",
    "recompile": "rimraf build-tsc && yarn -s tsc -b",
    "clean": "rimraf build-tsc && find . -type d -empty -delete",
    "...": "",
    "build": "yarn recompile && yarn build:fast",
    "build:fast": "yarn build:extra && yarn build:client",
    "build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-jawis.ts",
    "build:client": "webpack -o build/publish/javi/client -c packages/javi-client/webpack.javi.js",
    "....": "",
    "alpha:start": "concurrently --kill-others \"yarn -s alpha:start:alone\" \"yarn -s start:client\"",
    "alpha:start:alone": "echo alpha && node build-alpha/javi/javiServerMain",
    "alpha:build": "yarn recompile && yarn alpha:build:fast",
    "alpha:build:fast": "yarn alpha:build:extra && yarn alpha:build:paths && yarn alpha:build:client",
    "alpha:build:paths": "yarn -s tsc-alias -p tsconfig.alpha.paths.json",
    "alpha:build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-jawis-alpha.ts",
    "alpha:build:client": "webpack -o build-alpha/javi/client -c packages/javi-client/webpack.javi.js",
    ".....": "",
    "javi:min:start": "node build-jm/jm.js",
    "javi:min:build": "yarn javi:min:build:extra && yarn javi:min:build:client",
    "javi:min:build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-javi-min.ts",
    "javi:min:build:client": "webpack -o build-jm/client -c packages/javi-client/webpack.javi.js"
  },
  "dependencies": {
    "@jawis/console": "5.0.0-dev",
    "@jawis/jacs": "5.0.0-dev",
    "@jawis/jagov": "5.0.0-dev",
    "@jawis/javi": "5.0.0-dev",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/body-parser": "^1.19.2",
    "@types/copyfiles": "^2.4.1",
    "@types/cors": "^2.8.6",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.11",
    "@types/express-ws": "^3.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/fs-readdir-recursive": "^1.0.0",
    "@types/js-string-escape": "^1.0.1",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/selenium-webdriver": "^4.1.21",
    "@types/source-map-support": "^0.5.3",
    "@types/stack-trace": "^0.0.30",
    "@types/webpack-env": "^1.16.0",
    "@types/ws": "^8.5.2",
    "@types/yargs": "^17.0.7",
    "@types/yarnpkg__lockfile": "^1.1.5",
    "@yarnpkg/lockfile": "^1.1.0",
    "body-parser": "^1.20.1",
    "compressing": "^1.6.2",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "css-loader": "^6.7.1",
    "deep-equal": "^2.0.5",
    "del": "^6.0.0",
    "dir-compare": "^4.0.0",
    "echarts": "5.4.0",
