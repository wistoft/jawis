{
  "private": true,
  "description": "Jadev repository",
  "scripts": {
    "start": "concurrently --kill-others \"yarn -s javi\" \"yarn -s start:client\"",
    "start:client": "webpack serve  -c packages/dev/devClient/webpack.dev.js",
    "": "",
    "lint": "eslint packages",
    "lint:fix": "eslint --fix packages",
    "pretty": "prettier -l \"packages/**/{*.{ts,tsx,js,jsx,json,md,css,html},.prettierrc}\"",
    "pretty:fix": "yarn -s pretty --write",
    ".": "",
    "compile": "yarn -s tsc -b",
    "compile:clean": "rimraf build-tsc && yarn -s tsc -b",
    "..": "",
    "build": "gulp build && yarn -s build:extra && yarn -s build:client",
    "build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-javi.ts",
    "build:client": "webpack -o build/publish/javi/client/compiled -c packages/javi/client/webpack.javi.js",
    "...": "",
    "alpha:start": "concurrently --kill-others \"yarn -s alpha:start:alone\" \"yarn -s start:client\"",
    "alpha:start:alone": "echo alpha && node build-alpha/javi/server/javiServerMain",
    "alpha:build": "gulp buildAlpha && yarn -s alpha:build:paths && yarn -s alpha:build:extra && yarn -s alpha:build:client",
    "alpha:build:paths": "yarn -s tscpaths -p tsconfig.alpha.paths.json -s ./packages -o ./build-alpha | grep -vE \"replaced\"",
    "alpha:build:extra": "ts-node --transpile-only -r tsconfig-paths/register packages/dev/scripts/build-javi-alpha.ts",
    "alpha:build:client": "webpack -o build-alpha/javi/client/compiled -c packages/javi/client/webpack.javi.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@octokit/core": "^3.2.5",
    "@types/gulp": "^4.0.8",
    "@types/gulp-bump": "^2.8.1",
    "@types/gulp-if": "^0.0.33",
    "@types/gulp-json-editor": "^2.2.33",
    "@types/gulp-replace": "^0.0.31",
    "@types/gulp-sourcemaps": "^0.0.34",
    "@types/merge2": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@wistoft/javi": "^0.0.19",
    "concurrently": "^6.0.0",
    "del": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-if": "^3.0.0",
    "gulp-json-editor": "^2.5.4",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-ts-alias": "^1.3.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "lerna": "^3.22.1",
    "merge2": "^1.4.1",
    "npm-packlist": "1.1.12",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.2.6",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.3",
    "vinyl-named": "^1.1.0"
  },
  "version": "0.0.0"
}
